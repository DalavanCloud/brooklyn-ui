!!! 5
%html
    %head
        %title Brooklyn Webconsole - Dashboard
        %meta{:name=>"layout",:content=>"basic"}
        %script{:type=>"text/javascript", :src=>"http://maps.google.com/maps/api/js?sensor=false"}
        %g:javascript{:src=>"console/main.js"}
        %g:javascript{:src=>"console/jsTree.js"}
        %g:javascript{:src=>"console/tabs.js"}
        %g:javascript{:src=>"console/summary.js"}
        %g:javascript{:src=>"console/effectors.js"}
        %g:javascript{:src=>"console/activity.js"}
        %g:javascript{:src=>"console/sensors.js"}
        %g:javascript{:src=>"console/location.js"}
        %g:javascript{:src=>"console/debug-layout.js"}
        %g:javascript{:src=>"console/widget-dashboard.js"}
        %g:javascript{:src=>"jquery/jquery.corner.js"}
        %g:javascript{:src=>"jquery/jquery.jstree.js"}
    %body
        #tabs{:class=>"master-tabs"}
            %h1{:id=>"brooklyn-header"}
            %ul{:id=>"master-tabs-container"}
                %li{:id=>"master-filler-tab"}
                %li{:style=>"top:13px"}
                    %a{:href=>"#dashboard"}Dashboard
                %li{:style=>"top:13px"}
                    %a{:href=>"#debug"}Debug
                %li{:style=>"top:13px"}
                    %a{:href=>"#test"}Test
            #dashboard
                %g:render{:template=>"/tabs/dashboard"}
            #debug
                %g:render{:template=>"/tabs/debug"}
            #test
                %g:render{:template=>"/tabs/test"}
